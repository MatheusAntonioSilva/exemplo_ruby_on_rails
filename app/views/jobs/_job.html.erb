<% @jobs.each do |job| %>

<h3>

<% if job.premium %>
	<strong>(premium)</strong>
	<%= job.title %>
<% end %>

</h3>




<p>
  <%= pluralize(job.comments.size,  "comment") %> |
  <%= link_to 'Show', job %> 
  <%= link_to 'Edit', edit_job_path(job) %> 
  <%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %>
</p>

<%end%>